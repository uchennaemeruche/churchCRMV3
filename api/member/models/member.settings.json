    "phone_number": {
      "type": "string"
    },
    "birthdate": {
      "type": "date"
    },
    "employment_status": {
      "type": "enumeration",
      "enum": [
        "Employed",
        "SelfEmployed",
        "Unemployed"
      ],
      "required": true
    },
    "marital_status": {
      "type": "enumeration",
      "enum": [
        "Married",
        "Single",
        "WaitingOnGod"
      ]
    },
